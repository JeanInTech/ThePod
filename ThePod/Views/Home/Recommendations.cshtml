@model List<UserFeedback>

<img src="https://png2.cleanpng.com/sh/bbebdd4106dd999b26fe57d1564d30f6/L0KzQYm3U8I0N5D6j5H0aYP2gLBuTgBmaaF0fJ9sbHnzPbL5lL1xbZIyTdNrNUDnQIe8hsA0O2MzSac8NEG8QIa4VcIyQGI5T6s5M0i8QnB3jvc=/kisspng-peapod-clip-art-pea-5ab50d065f0332.1534190515218147903892.png" />


<style>
    body {
        background-color: #ff006e;
    }

    table {
        border-collapse: initial;
        margin-left: auto;
        margin-right: auto;
        width: 75%;
    }

    td {
        background-color: white;
        border-bottom: solid 20px #b6ff00;
        border-top: solid 1px #0094ff;
        box-shadow: 1px 1px 1px #000000;
        padding: 15px;
        text-align: center;
        width: 15%;
    }

    tr td:first-child {
        border-left: solid 5px #10909A;
    }

    tr td:last-child {
        border-right: solid 2px #10909A;
    }

    tr:first-child td:first-child {
        border-top-left-radius: 4px;
    }

    tr:first-child td:last-child {
        border-top-right-radius: 4px;
    }

    tr:last-child td:first-child {
        border-bottom-left-radius: 4px;
    }

    tr:last-child td:last-child {
        border-bottom-right-radius: 4px;
    }
</style>





<table class=" =styled-table">

    @foreach (var u in Model)

    {
        <thead>
            <tr>

                <th>Rating</th>
                <th>Review</th>
                <th>Tags</th>
            </tr>
        </thead>
        <tbody>
            <tr>
            </tr>
            <tr>

                <td><p>@u.Rating</p></td>
                <td><p>@u.Review</p></td>
                <td><p>@u.Tags</p></td>
            </tr>
        </tbody>
    }

</table>
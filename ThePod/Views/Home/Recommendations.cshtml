@model List<UserFeedback>



<style>
    body {
        background-color: #ff006e;
    }

    table {
        border-collapse: initial;
        margin-left: auto;
        margin-right: auto;
        width: 75%;
    }

    td {
        background-color: white;
        border-bottom: solid 20px #b6ff00;
        border-top: solid 1px #0094ff;
        box-shadow: 1px 1px 1px #000000;
        padding: 15px;
        text-align: center;
        width: 15%;
    }

    tr td:first-child {
        border-left: solid 5px #10909A;
    }

    tr td:last-child {
        border-right: solid 2px #10909A;
    }

    tr:first-child td:first-child {
        border-top-left-radius: 4px;
    }

    tr:first-child td:last-child {
        border-top-right-radius: 4px;
    }

    tr:last-child td:first-child {
        border-bottom-left-radius: 4px;
    }

    tr:last-child td:last-child {
        border-bottom-right-radius: 4px;
    }
</style>





<table class=" =styled-table">

    @foreach (var u in Model)

    {
        <thead>
            <tr>

                <th>Rating</th>
                <th>Review</th>
                <th>Tags</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <tr>
            </tr>
            <tr>

                <td><p>@u.Rating</p></td>
                <td><p>@u.Review</p></td>
                <td><p>@u.Tags</p></td>
                <td><p><img src="@u.ImageUrl"/></p></td>
            </tr>
        </tbody>
    }

</table>